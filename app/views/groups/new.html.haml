%h2.page_title 
  Create a new Group
  %ul
    %li= link_to "Back to the Groups list", objects_url
    %li= link_to "Back to the homepage", home_url

.form_holder
  = error_messages_for :group

  - form_for(current_object, :url => objects_url) do |f|
    = render :partial => "form", :locals => {:f => f}
    %p.submit
      = submit_tag("Create group")
      = link_to("Cancel", :back, :class => "alternative_button")
